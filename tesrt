    }), "Main")
    transparentButton.Parent = transparentToggleWrapper

    transparentCheckbox = AddThemeObject(SetProps(MakeElement("Frame"), {
        Size = UDim2.new(1, 0, 1, 0),
        BackgroundTransparency = 1
    }), "Text")
