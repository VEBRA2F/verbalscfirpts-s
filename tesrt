    videoLocationToggled = false

    for _, opt in ipairs(videoLocationOptions) do
        optBtn = AddThemeObject(Create("TextButton", {
            Name = opt,
            Size = UDim2.new(1, 0, 0, 24),
            Text = "",
            ZIndex = 51
        }), "Main")
